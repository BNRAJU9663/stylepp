mkdir ../../stylepp_maintained

git clone git://gcc.gnu.org/git/gcc.git ../../stylepp_maintained/gcc
git clone hg::http://gmplib.org:8000/gmp ../../stylepp_maintained/gmp
git svn clone svn://scm.gforge.inria.fr/svn/mpfr/trunk ../../stylepp_maintained/mpfr
git svn clone svn://scm.gforge.inria.fr/svnroot/mpc/trunk ../../stylepp_maintained/mpc
git clone https://github.com/madler/zlib ../../stylepp_maintained/zlib
git clone https://github.com/ivmai/bdwgc.git ../../stylepp_maintained/boehm-gc
git clone https://github.com/git/git.git ../../stylepp_maintained/git
git clone git://github.com/atgreen/libffi.git ../../stylepp_maintained/libffi
git clone git://git.savannah.gnu.org/libtool.git ../../stylepp_maintained/libtool
git clone git://git.savannah.gnu.org/classpath.git ../../stylepp_maintained/classpath


cd ../../stylepp_maintained; cvs -z3 -d:pserver:anonymous@cvs.savannah.gnu.org:/sources/aspell co aspell
