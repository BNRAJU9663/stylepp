DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PID=$BASHPID
PATH=$PATH:$DIR/../bin:$DIR

echo > /tmp/misspells.$PID
for I in `find -regex ".*[.][ch]\$"`; do
  for WORD in `stylepp_extract_comment_spellcheck $I < $I | aspell list`; do
    echo $WORD >>/tmp/misspells.$PID
  done
done

cat /tmp/misspells.$PID | sort | uniq --count |  sort  -n -r 
#grep -v "[[:upper:]]" 

# Cleanup.
rm /tmp/misspells.$PID
